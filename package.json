{
  "name": "linkedin-js-tips-poster",
  "version": "1.0.0",
  "description": "A Node.js application that posts daily JavaScript and React tips to LinkedIn.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "client": "cd frontend && npm start",
    "build-client": "cd frontend && npm run build",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "form-data": "^4.0.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.7"
  },
  "author": "Your Name",
  "license": "MIT"
}
